{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "jest": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/*.test.js",
          "**/*.test.jsx",
          "**/*.spec.js",
          "**/*.spec.jsx",
          "**/*.stories.jsx"
        ]
      }
    ]
  }
}
